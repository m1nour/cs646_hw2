<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ClassesActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <CheckBox
            android:id="@+id/checkBox596"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:text="@string/cs596" />

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView596Title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs596_title" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView596Room"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs596_room" />

                <TextView
                    android:id="@+id/textView596Time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs596_time" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/checkBox605"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/cs605_no" />

                <TextView
                    android:id="@+id/textView605Title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs605_title" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textView605Room"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs605_room" />

                <TextView
                    android:id="@+id/textView605Time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs605_time" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/checkBox615"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/cs615_no" />

                <TextView
                    android:id="@+id/textView615Title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs615_title" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textView615Room"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs615_room" />

                <TextView
                    android:id="@+id/textView615Time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs615_time" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/checkBox646"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/cs646_no" />

                <TextView
                    android:id="@+id/textView646Title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs646_title" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textView646Room"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs646_room" />

                <TextView
                    android:id="@+id/textView646Time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs646_time" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/checkBox696"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/cs696_no" />

                <TextView
                    android:id="@+id/textView696Title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs696_title" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textView696Room"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs696_room" />

                <TextView
                    android:id="@+id/textView696Time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs696_time" />
            </TableRow>

        </TableLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="380dp"
            android:layout_weight="1" />

        <Button
            android:id="@+id/homeButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/home" />

        <Button
            android:id="@+id/infoButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/info" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>